   "source": [
    "## 7. Feature engineering - time features + lags + rolling"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "2db9a64e",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-09-18T12:16:27.289967Z",
     "iopub.status.busy": "2025-09-18T12:16:27.289655Z",
     "iopub.status.idle": "2025-09-18T12:16:27.360181Z",
     "shell.execute_reply": "2025-09-18T12:16:27.359392Z"
    },
    "papermill": {
     "duration": 0.083628,
     "end_time": "2025-09-18T12:16:27.361959",
     "exception": false,
     "start_time": "2025-09-18T12:16:27.278331",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "# basic time feature\n",
    "\n",
    "df['Year'] = df['Date'].dt.year\n",
    "df['Month'] = df['Date'].dt.month\n",
    "df['Week'] = df['Date'].dt.isocalendar().week\n",
    "df['DayOfWeek'] = df['Date'].dt.dayofweek\n",
    "df['IsHoliday'] = df['IsHoliday'].astype(int)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "47072dae",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-09-18T12:16:27.382230Z",
     "iopub.status.busy": "2025-09-18T12:16:27.381898Z",
     "iopub.status.idle": "2025-09-18T12:16:27.579526Z",
     "shell.execute_reply": "2025-09-18T12:16:27.578498Z"
    },
    "papermill": {
     "duration": 0.209928,
     "end_time": "2025-09-18T12:16:27.581373",
     "exception": false,
     "start_time": "2025-09-18T12:16:27.371445",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "# 7.1 - create lags and rolling per store-dept\n",
    "\n",
    "lag_list = [1,2,3,4,12]   # weeks\n",
    "for lag in lag_list:\n",
    "    df[f'lag_{lag}'] = df.groupby(['Store','Dept'])['Weekly_Sales'].shift(lag)\n",
    "\n",
    "df['rmean_4'] = df.groupby(['Store','Dept'])['Weekly_Sales'].shift(1).rolling(window=4).mean()\n",
    "df['rmean_12'] = df.groupby(['Store','Dept'])['Weekly_Sales'].shift(1).rolling(window=12).mean()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "db2ef7a3",
   "metadata": {
    "execution": {
